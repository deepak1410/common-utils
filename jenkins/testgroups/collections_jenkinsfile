pipeline {
  agent any
  stages {

    stage("Build") {
      steps() {
        sh "mvn clean package -B -DskipTests"
      }
    }

    stage("Test") {
      steps() {
        sh "mvn test -Dtest=com.dpk.list.**.*Test,com.dpk.set.**.*Test"
      }
      post {
        always {
          junit "target/surefire-reports/*.xml"
        }
      }
    }

  }
}